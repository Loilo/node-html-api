{
  "name": "@loilo/html-api",
  "version": "0.1.1",
  "description": "Makes creating an HTML API a breeze",
  "main": "dist/cjs.es2015.js",
  "devDependencies": {
    "buble": "^0.15.2",
    "del-cli": "^1.1.0",
    "make-dir-cli": "^1.0.0",
    "standard": "^10.0.2",
    "uglify-es": "^3.0.25"
  },
  "scripts": {
    "test": "standard",
    "prepublish": "npm run build",
    "build-prepare": "make-dir dist",
    "build-polyfilled": "node build/merge --sep-newline src/polyfill.map.js src/html-api.js > tmp-polyfilled.js",
    "build-es5": "buble src/html-api.js --yes dangerousForOf --no modules > tmp-transpiled.js",
    "build-es5-full": "buble tmp-polyfilled.js --yes dangerousForOf --no modules > tmp-transpiled-full.js",
    "build-browser-es5": "rollup -c build/config.rollup.browser.es5.js | uglifyjs --compress --mangle > dist/browser.min.js",
    "build-browser-es2015": "rollup -c build/config.rollup.browser.es2015.js | uglifyjs --compress --mangle > dist/browser.es2015.min.js",
    "build-browser-module": "uglifyjs tmp-transpiled.js --compress --mangle > dist/browser.module.min.js",
    "build-node-es5": "rollup -c build/config.rollup.node.es5.js",
    "build-node-es2015": "rollup -c build/config.rollup.node.es2015.js",
    "build-cleanup": "del-cli tmp-transpiled.js; del-cli tmp-transpiled-full.js; del-cli tmp-polyfilled.js",
    "build": "npm run build-prepare && npm run build-polyfilled && npm run build-es5 && npm run build-es5-full && npm run build-browser-es5 && npm run build-browser-es2015 && npm run build-browser-module && npm run build-node-es5 && npm run build-node-es2015 && npm run build-cleanup"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Loilo/node-html-api.git"
  },
  "keywords": [
    "html",
    "api"
  ],
  "author": "Florian Reuschel <florian@loilo.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Loilo/node-html-api/issues"
  },
  "homepage": "https://github.com/Loilo/node-html-api#readme",
  "standard": {
    "ignore": [
      "dist/",
      "build/"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
